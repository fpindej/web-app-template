{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "Database": "Host=localhost;Port={INIT_DB_PORT};Database={INIT_PROJECT_SLUG};Username=admin;Password=pwd"
  },
  "Redis": {
    "Enabled": true,
    "ConnectionString": "localhost:{INIT_REDIS_PORT}",
    "Password": "redis-dev-password",
    "UseSsl": false,
    "DefaultDatabase": 0,
    "InstanceName": "{INIT_PROJECT_SLUG}:",
    "DefaultExpiration": "00:10:00",
    "ConnectTimeoutMs": 5000,
    "SyncTimeoutMs": 5000,
    "AsyncTimeoutMs": 5000,
    "AbortOnConnectFail": true,
    "ConnectRetry": 3,
    "KeepAliveSeconds": 60,
    "InMemorySizeLimit": 1024
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    }
  },
  "Cors": {
    "AllowAllOrigins": true,
    "AllowedOrigins": [],
    "PolicyName": "DefaultCorsPolicy"
  }
}
